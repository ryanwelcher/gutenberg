{
	"apiVersion": 2,
	"name": "core/more-from-author",
	"title": "More From Author",
	"category": "widgets",
	"description": "Display a list of content from the same author",
	"keywords": [ "more posts", "author" ],
	"textdomain": "default",
	"attributes": {
		"title": {
			"type": "string",
			"default": "More from this author"
		},
		"postCount": {
			"type": "number",
			"default": 3
		}
	},
	"usesContext": [ "postType", "postId" ],
	"supports": {
		"align": true,
		"html": false
	},
	"editorStyle": "wp-block-latest-posts-editor",
	"style": "wp-block-latest-posts"
}
